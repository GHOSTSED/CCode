{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "C/C++: gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"${file}",
				"${workspaceFolder}/src/DList.c",
				"${workspaceFolder}/include/DMonitor.h",
				"-o",
				"${workspaceFolder}/exe/${fileBasenameNoExtension}",
			],
			// "options": {
			// 	"cwd": "${workspaceFolder}"
			// },
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}